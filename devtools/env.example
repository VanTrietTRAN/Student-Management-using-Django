# Project namespace (defaults to the current folder name if not set)
#COMPOSE_PROJECT_NAME=myproject

MYSQL_ROOT_PASSWORD=MyPassword
# Port to expose MySQL to the host
MYSQL_PORT=3308

# Port to expose Redis to the host
REDIS_PORT=6382

# Port to Mongodb  to the host
MONGO_PORT=27018
# Username and password for mongo db
MONGO_INITDB_ROOT_USERNAME=root
MONGO_INITDB_ROOT_PASSWORD=MyPassword

# Port to Mongo Express  to the host
MONGO_EXPRESS_PORT=8091
# User name and password for Mongo express
ME_CONFIG_BASICAUTH_USERNAME=express
ME_CONFIG_BASICAUTH_PASSWORD=MyPassword

# Port to DynamoDB  to the host
DYNAMODB_PORT=6920

# Port to Rasa  to the host
RASA_PORT=5008

# Version of Elastic products
STACK_VERSION=8.7.1

# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9300
# Password for the 'elastic' user (at least 6 characters)
ELASTIC_PASSWORD=MyPassword


# Port to expose Kibana to the host
KIBANA_PORT=5801
# Password for the 'kibana_system' user (at least 6 characters)
KIBANA_PASSWORD=MyPassword

# Set the cluster name
CLUSTER_NAME=docker-cluster


# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic
#LICENSE=trial


# Increase or decrease based on the available host memory (in bytes)
